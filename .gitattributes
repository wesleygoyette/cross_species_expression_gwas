# This file configures Git LFS tracking for large files in the project.
data/regland.sqlite filter=lfs diff=lfs merge=lfs -text
data/regland.sqlite-shm filter=lfs diff=lfs merge=lfs -text
data/regland.sqlite-wal filter=lfs diff=lfs merge=lfs -text
data/expression_tpm.tsv filter=lfs diff=lfs merge=lfs -text
data/enhancer_labels.tsv filter=lfs diff=lfs merge=lfs -text
*.gtf filter=lfs diff=lfs merge=lfs -text
*.gtf.gz filter=lfs diff=lfs merge=lfs -text
*.bed.gz filter=lfs diff=lfs merge=lfs -text
*.parquet filter=lfs diff=lfs merge=lfs -text
*.sqlite filter=lfs diff=lfs merge=lfs -text
*.tsv filter=lfs diff=lfs merge=lfs -text